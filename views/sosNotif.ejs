<%- include("partials/header"); -%>

    <!--external css-->
    <link href="/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" href="/lib/advanced-datatable/css/DT_bootstrap.css" />
    <!-- Custom styles for this template -->
    <link href="/css/style.css" rel="stylesheet">
    <link href="/css/style-responsive.css" rel="stylesheet">


    <section id="main-content">
        <section class="wrapper">
            <h3>Notification > Les notifications des SOS</h3>
            <div class="row mb">
                <!-- page start-->
                <div class="content-panel">
                    <div class="adv-table">
                        <br>
                        <table cellpadding="0" cellspacing="0" border="0" id="example" class="display table table-bordered">
                            <thead>
                                <tr>
                                    <th style="text-align: center;">NA</th>
                                    <th style="text-align: center;">ID_VMS</th>
                                    <th class="hidden-phone" style="text-align: center;">RC</th>
                                    <th class="hidden-phone" style="text-align: center;">DA</th>
                                    <th class="hidden-phone" style="text-align: center;">TI</th>
                                    <th class="hidden-phone" style="text-align: center;">LT</th>
                                    <th class="hidden-phone" style="text-align: center;">LG</th>
                                    <th class="hidden-phone" style="text-align: center;">CO</th>
                                    <th class="hidden-phone" style="text-align: center;">SP</th>
                                    <th class="hidden-phone" style="text-align: center;">COM</th>
                                    <th class="hidden-phone" style="text-align: center;">TM</th>

                                </tr>
                            </thead>

                            <tbody>
                                <% SOSnotifications.forEach(function(sos) { %>

                                    <tr class="gradeX">

                                        <td style="text-align: center;font-size: medium;">
                                            <a href="/navire/navire/<%= sos.ID_VMS%>" style="color:gray;">
                                                <%= sos.NA %>
                                            </a>
                                        </td>
                                        <td class="hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= sos.ID_VMS %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= sos.RC %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">

                                            <%= sos.DA[6]%>
                                                <%= sos.DA[7]%> -
                                                    <%= sos.DA[4]%>
                                                        <%= sos.DA[5]%> -
                                                            <%= sos.DA[0]%>
                                                                <%= sos.DA[1]%>
                                                                    <%= sos.DA[2]%>
                                                                        <%= sos.DA[3]%>

                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= sos.TI %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= sos.LT %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= sos.LG %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= sos.CO %>
                                        </td>

                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= sos.SP %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= sos.COM %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= sos.TM %>
                                        </td>

                                    </tr>

                                    <% }); %>

                            </tbody>
                        </table>

                    </div>
                </div>
                <!-- page end-->
            </div>
            <!-- /row -->
            <!-- /wrapper -->
        </section>
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>

        <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js" defer></script>

        <script>
            $(document).ready(function() {
                $('#example').DataTable();
            });
        </script>


        <%- include("partials/footer"); -%>