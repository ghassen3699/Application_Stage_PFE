<%- include("../partials/header"); -%>

    <section id="main-content">
        <section class="wrapper">



            <div class="row mt">
                <div class="col-lg-12">


                    <div class="form-panel">
                        <h3 style="text-align: center;"> Navire :
                            <%= name %>


                        </h3>
                        <br>
                        <br>
                        <div class=" form">
                            <form class="cmxform form-horizontal style-form" id="commentForm" method="post">

                                <% navire.forEach(function(n) { %>


                                    <div class="form-group ">
                                        <label for="cemail" class="control-label col-lg-2" style="font-size: medium;">NA </label>
                                        <div class="col-lg-5">
                                            <input value="<%= n.NA %>" class="form-control " id="cemail" type="text" name="NA" />
                                        </div>
                                    </div>
                                    <br>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">ID_VMS </label>
                                        <div class="col-lg-10">
                                            <input value="<%= n.ID_VMS %>" class="form-control " id="curl" type="text" name="ID_VMS" required />
                                        </div>
                                    </div>
                                    <br>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">REG_ID </label>
                                        <div class="col-lg-5">
                                            <input value="<%= n.REG_ID %>" class="form-control " id="curl" type="text" name="REG_ID" required />
                                        </div>
                                    </div>
                                    <br>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">IMEI </label>
                                        <div class="col-lg-5">
                                            <input value="<%= n.IMEI %>" class="form-control " id="curl" type="text" name="IMEI" required />
                                        </div>
                                    </div>
                                    <br>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">ICCID </label>
                                        <div class="col-lg-10">
                                            <input value="<%= n.ICCID %>" class="form-control " id="curl" type="text" name="ICCID" required />
                                        </div>
                                    </div>


                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">RC </label>
                                        <div class="col-lg-10">
                                            <input value="<%= n.RC %>" class="form-control " id="curl" type="text" name="RC" required />
                                        </div>
                                    </div>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">KEY_AES</label>
                                        <div class="col-lg-10">
                                            <input value="<%= n.KEY_AES %>" class="form-control " id="curl" type="text" name="KEY_AES" required />
                                        </div>
                                    </div>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">DABEG</label>
                                        <div class="col-lg-10">
                                            <input value="<%= n.DABeg %>" class="form-control " id="curl" type="text" name="DABEG" required />
                                        </div>
                                    </div>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">DAEND</label>
                                        <div class="col-lg-10">
                                            <input value="<%= n.DAEnd %>" class="form-control " id="curl" type="text" name="DAEND" required />
                                        </div>
                                    </div>


                                    <div class="form-group">
                                        <div class="col-lg-offset-2 col-lg-10">
                                            <button class="btn btn-theme" type="submit">Enregistrer</button>
                                            <a href="/home" class="btn btn-theme04" type="button">Annuler</a>
                                        </div>
                                    </div>

                                    <% }); %>
                            </form>
                        </div>
                    </div>
                </div>
            </div>

        </section>
    </section>

    <%- include("../partials/footer"); -%>