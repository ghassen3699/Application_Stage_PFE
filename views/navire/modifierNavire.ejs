<%- include("../partials/header"); -%>
    <section id="main-content">
        <section class="wrapper">



            <div class="row mt">
                <div class="col-lg-13">
                    <div class="custom-box">
                        <% if (msg !== ""){%>
                            <div class="alert alert-success"><b>DÃ©sole! &nbsp;</b>
                                <%=msg%>.</div>
                            <%}%>

                                <h3 style="text-align: center;">
                                    <%= name %>
                                </h3>
                    </div>
                    <div class="form-panel">


                        <div class="form">
                            <% navireData.forEach(navire =>{ %>


                                <form class="cmxform form-horizontal style-form" id="commentForm" method="post">
                                    <div class="form-group ">
                                        <label for="cemail" class="control-label col-lg-2" style="font-size: medium;">Nom du navire :</label>
                                        <div class="col-lg-5">
                                            <input value="<%= navire['NA'] %>" class="form-control " id="cemail" type="text" name="NA" />
                                        </div>
                                    </div>
                                    <br>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">ID_VMS du navire :</label>
                                        <div class="col-lg-10">
                                            <input value="<%= navire['ID_VMS'] %>" class="form-control " id="curl" type="text" name="ID_VMS" disabled required />
                                        </div>
                                    </div>
                                    <br>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">REG_ID du navire :</label>
                                        <div class="col-lg-5">
                                            <input value="<%= navire['REG_ID'] %>" class="form-control " id="curl" type="text" name="REG_ID" disabled required />
                                        </div>
                                    </div>
                                    <br>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">IMEI du navire :</label>
                                        <div class="col-lg-5">
                                            <input value="<%= navire['IMEI'] %>" class="form-control " id="curl" type="text" name="IMEI" required />
                                        </div>
                                    </div>
                                    <br>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">ICCID du navire :</label>
                                        <div class="col-lg-10">
                                            <input value="<%= navire['ICCID'] %>" class="form-control " id="curl" type="text" name="ICCID" required />
                                        </div>
                                    </div>


                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">RC du navire :</label>
                                        <div class="col-lg-10">
                                            <input value="<%= navire['RC'] %>" class="form-control " id="curl" type="text" name="RC" required />
                                        </div>
                                    </div>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">KEY_AES du navire :</label>
                                        <div class="col-lg-10">
                                            <input value="<%= navire['KEY_AES'] %>" class="form-control " id="curl" type="text" name="KEY_AES" required />
                                        </div>
                                    </div>



                                    <div class="form-group">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">Date debut d'abonnement :</label>
                                        <div class="col-md-3 col-xs-11">
                                            <div class="input-append date dpYears">
                                                <input type="date" value="<%= navire['DABeg'][0] %><%= navire['DABeg'][1] %><%= navire['DABeg'][2] %><%= navire['DABeg'][3] %>-<%= navire['DABeg'][4] %><%= navire['DABeg'][5] %>-<%= navire['DABeg'][6] %><%= navire['DABeg'][7] %>" id="bday" name="DABEG"
                                                    size="16" class="form-control" required pattern="[0-9]{4}-[0-9]{2}-[0-9]{2}">
                                            </div>
                                        </div>
                                    </div>



                                    <div class="form-group">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">Date fin d'abonnement :</label>
                                        <div class="col-md-3 col-xs-11">
                                            <div class="input-append date dpYears">
                                                <input type="date" value="<%= navire['DAEnd'][0] %><%= navire['DAEnd'][1] %><%= navire['DAEnd'][2] %><%= navire['DAEnd'][3] %>-<%= navire['DAEnd'][4] %><%= navire['DAEnd'][5] %>-<%= navire['DAEnd'][6] %><%= navire['DAEnd'][7] %>" id="bday" name="DAEND"
                                                    size="16" class="form-control" required pattern="[0-9]{4}-[0-9]{2}-[0-9]{2}">
                                            </div>
                                        </div>
                                    </div>


                                    <div class="custom-box">
                                        <h3 style="text-align: center;">
                                            <button class="btn btn-theme" type="submit" style="font-size: medium;">Enregistrer</button>
                                            <a href="/navire/navires" class="btn btn-theme04" type="button" style="font-size: medium;">Annuler</a>
                                        </h3>
                                    </div>

                                </form>
                                <%}) %>

                        </div>
                    </div>
                </div>
            </div>

        </section>
    </section>


    <%- include("../partials/footer"); -%>