<%- include("../partials/header"); -%>

    <section id="main-content">
        <section class="wrapper">



            <div class="row mt">
                <div class="col-lg-12">

                    <%= msg %>
                        <div class="form-panel">
                            <h3 style="text-align: center;"> Navire :
                                <%= name %>


                            </h3>
                            <br>
                            <br>
                            <div class=" form">
                                <% navireData.forEach(navire =>{ %>


                                    <form class="cmxform form-horizontal style-form" id="commentForm" method="post">
                                        <div class="form-group ">
                                            <label for="cemail" class="control-label col-lg-2" style="font-size: medium;">NA </label>
                                            <div class="col-lg-5">
                                                <input value="<%= navire['NA'] %>" class="form-control " id="cemail" type="text" name="NA" />
                                            </div>
                                        </div>
                                        <br>

                                        <div class="form-group ">
                                            <label for="curl" class="control-label col-lg-2" style="font-size: medium;">ID_VMS </label>
                                            <div class="col-lg-10">
                                                <input value="<%= navire['ID_VMS'] %>" class="form-control " id="curl" type="text" name="ID_VMS" disabled required />
                                            </div>
                                        </div>
                                        <br>

                                        <div class="form-group ">
                                            <label for="curl" class="control-label col-lg-2" style="font-size: medium;">REG_ID </label>
                                            <div class="col-lg-5">
                                                <input value="<%= navire['REG_ID'] %>" class="form-control " id="curl" type="text" name="REG_ID" disabled required />
                                            </div>
                                        </div>
                                        <br>

                                        <div class="form-group ">
                                            <label for="curl" class="control-label col-lg-2" style="font-size: medium;">IMEI </label>
                                            <div class="col-lg-5">
                                                <input value="<%= navire['IMEI'] %>" class="form-control " id="curl" type="text" name="IMEI" required />
                                            </div>
                                        </div>
                                        <br>

                                        <div class="form-group ">
                                            <label for="curl" class="control-label col-lg-2" style="font-size: medium;">ICCID </label>
                                            <div class="col-lg-10">
                                                <input value="<%= navire['ICCID'] %>" class="form-control " id="curl" type="text" name="ICCID" required />
                                            </div>
                                        </div>


                                        <div class="form-group ">
                                            <label for="curl" class="control-label col-lg-2" style="font-size: medium;">RC </label>
                                            <div class="col-lg-10">
                                                <input value="<%= navire['RC'] %>" class="form-control " id="curl" type="text" name="RC" required />
                                            </div>
                                        </div>

                                        <div class="form-group ">
                                            <label for="curl" class="control-label col-lg-2" style="font-size: medium;">KEY_AES</label>
                                            <div class="col-lg-10">
                                                <input value="<%= navire['KEY_AES'] %>" class="form-control " id="curl" type="text" name="KEY_AES" required />
                                            </div>
                                        </div>



                                        <div class="form-group">
                                            <label for="curl" class="control-label col-lg-2" style="font-size: medium;">DABEG</label>
                                            <div class="col-md-3 col-xs-11">
                                                <div class="input-append date dpYears">
                                                    <input type="date" value="<%= navire['DABeg'][0] %><%= navire['DABeg'][1] %><%= navire['DABeg'][2] %><%= navire['DABeg'][3] %>-<%= navire['DABeg'][4] %><%= navire['DABeg'][5] %>-<%= navire['DABeg'][6] %><%= navire['DABeg'][7] %>" id="bday" name="DABEG"
                                                        size="16" class="form-control" required pattern="[0-9]{4}-[0-9]{2}-[0-9]{2}">
                                                </div>
                                            </div>
                                        </div>



                                        <div class="form-group">
                                            <label for="curl" class="control-label col-lg-2" style="font-size: medium;">DAEND</label>
                                            <div class="col-md-3 col-xs-11">
                                                <div class="input-append date dpYears">
                                                    <input type="date" value="<%= navire['DAEnd'][0] %><%= navire['DAEnd'][1] %><%= navire['DAEnd'][2] %><%= navire['DAEnd'][3] %>-<%= navire['DAEnd'][4] %><%= navire['DAEnd'][5] %>-<%= navire['DAEnd'][6] %><%= navire['DAEnd'][7] %>" id="bday" name="DAEND"
                                                        size="16" class="form-control" required pattern="[0-9]{4}-[0-9]{2}-[0-9]{2}">
                                                </div>
                                            </div>
                                        </div>


                                        <div class="form-group">
                                            <div class="col-lg-offset-2 col-lg-10">
                                                <button class="btn btn-theme" type="submit">Enregistrer</button>
                                                <a href="/home" class="btn btn-theme04" type="button">Annuler</a>
                                            </div>
                                        </div>

                                    </form>
                                    <%}) %>

                            </div>
                        </div>
                </div>

        </section>
    </section>

    <%- include("../partials/footer"); -%>