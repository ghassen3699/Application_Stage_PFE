<%- include("../partials/header"); -%>



    <section id="main-content">
        <section class="wrapper">



            <div class="row mt">
                <div class="col-lg-13">
                    <div class="custom-box">
                        <% if (msg !== ""){%>
                            <script>
                                alert('<%= msg%>')
                            </script>

                            <%}%>

                                <h3 style="text-align: center;">Ajouter Navire</h3>
                    </div>
                    <div class="form-panel">


                        <div class=" form">
                            <form id="form" class="cmxform form-horizontal style-form" id="commentForm" method="post" action="/navire/ajouter">
                                <br>


                                <div class="form-group ">
                                    <label for="cemail" class="control-label col-lg-2" style="font-size: medium;">Nom du navire :</label>
                                    <div class="col-lg-5">
                                        <input class="form-control" id="NA" placeholder="Le Nom Du Navire ..." id="curl" type="text" name="NA" required />
                                    </div>
                                </div>
                                <br>

                                <div class="form-group ">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">ID_VMS du navire :</label>
                                    <div class="col-lg-10">
                                        <input class="form-control" id="ID_VMS" placeholder="Exemple ( VMS0000 ...)" id="curl" type="text" name="ID_VMS" required />
                                    </div>
                                </div>
                                <br>



                                <div class="form-group ">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">IMEI du navire :</label>
                                    <div class="col-lg-5">
                                        <input class="form-control" id="IMEI" placeholder="Exemple (123456789....)" id="curl" type="text" name="IMEI" required />
                                    </div>
                                </div>
                                <br>

                                <div class="form-group ">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">ICCID du navire :</label>
                                    <div class="col-lg-10">
                                        <input class="form-control" id="CURL" placeholder="Exemple (123456789 ....)" id="curl" type="text" name="ICCID" required />
                                    </div>
                                </div>


                                <div class="form-group ">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">RC du navire :</label>
                                    <div class="col-lg-10">
                                        <input class="form-control" id="RC" placeholder="Exemple (3A1234 ....)" id="curl" type="text" name="RC" required />
                                    </div>
                                </div>

                                <div class="form-group ">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">KEY_AES du navire :</label>
                                    <div class="col-lg-10">
                                        <input class="form-control" id="KEY_AES" placeholder=" Exemple (706F656C736500000000000000001111 ....)" id="curl" type="text" name="KEY_AES" required />
                                    </div>
                                </div>


                                <div class="form-group">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">Date debut d'abonnement :</label>
                                    <div class="col-md-3 col-xs-11">
                                        <div class="input-append date dpYears">
                                            <input type="date" lang="fr-CA" value="" id="dateDebut" name="DABEG" size="16" class="form-control" required pattern="[0-9]{4}-[0-9]{2}-[0-9]{2}">
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">Date fin d'abonnement :</label>
                                    <div class="col-md-3 col-xs-11">
                                        <div class="input-append date dpYears">
                                            <input type="date" lang="fr-CA" value="" id="DateFin" name="DAEND" size="16" class="form-control" required pattern="[0-9]{4}-[0-9]{2}-[0-9]{2}">
                                        </div>
                                    </div>
                                </div>



                                <div class="custom-box">
                                    <h3 style="text-align: center;">
                                        <button class="btn btn-theme" type="submit" style="font-size: medium;">Enregistrer</button>
                                        <a href="/home" class="btn btn-theme04" type="button" style="font-size: medium;">Annuler</a>
                                    </h3>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </section>
    <script type="text/javascript" src="/js/pickerDateForm.js"></script>








    <%- include("../partials/footer"); -%>