<%- include("../partials/header"); -%>
    <!--external css-->


    <section id="main-content">
        <section class="wrapper">



            <div class="row mt">
                <div class="col-lg-13">
                    <div class="custom-box">
                        <% if (msg !== ""){%>
                            <div class="alert alert-success"><b>DÃ©sole! &nbsp;</b>
                                <%=msg%>.</div>
                            <%}%>

                                <h3 style="text-align: center;">Ajouter Navire</h3>
                    </div>
                    <div class="form-panel">


                        <div class=" form">
                            <form class="cmxform form-horizontal style-form" id="commentForm" method="post" action="/navire/ajouter">
                                <br>


                                <div class="form-group ">
                                    <label for="cemail" class="control-label col-lg-2" style="font-size: medium;">Le nom de la navire :</label>
                                    <div class="col-lg-5">
                                        <input class="form-control" placeholder="Le Nom De La Navire ..." id="curl" type="text" name="NA" required />
                                    </div>
                                </div>
                                <br>

                                <div class="form-group ">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">ID_VMS de la navire :</label>
                                    <div class="col-lg-10">
                                        <input class="form-control" placeholder="VMS0000 ..." id="curl" type="text" name="ID_VMS" required />
                                    </div>
                                </div>
                                <br>



                                <div class="form-group ">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">IMEI de la navire :</label>
                                    <div class="col-lg-5">
                                        <input class="form-control" placeholder="123456789...." id="curl" type="text" name="IMEI" required />
                                    </div>
                                </div>
                                <br>

                                <div class="form-group ">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">ICCID de la navire :</label>
                                    <div class="col-lg-10">
                                        <input class="form-control" placeholder="123456789 ...." id="curl" type="text" name="ICCID" required />
                                    </div>
                                </div>


                                <div class="form-group ">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">RC de la navire :</label>
                                    <div class="col-lg-10">
                                        <input class="form-control" placeholder="3A1234 ...." id="curl" type="text" name="RC" required />
                                    </div>
                                </div>

                                <div class="form-group ">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">KEY_AES de la navire :</label>
                                    <div class="col-lg-10">
                                        <input class="form-control" placeholder="706F656C736500000000000000001111 ...." id="curl" type="text" name="KEY_AES" required />
                                    </div>
                                </div>


                                <div class="form-group">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">Date debut d'abonnement :</label>
                                    <div class="col-md-3 col-xs-11">
                                        <div class="input-append date dpYears">
                                            <input type="date" value="" id="bday" name="DABEG" size="16" class="form-control" required pattern="[0-9]{4}-[0-9]{2}-[0-9]{2}">
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="curl" class="control-label col-lg-2" style="font-size: medium;">Date fin d'abonnement :</label>
                                    <div class="col-md-3 col-xs-11">
                                        <div class="input-append date dpYears">
                                            <input type="date" value="" id="bday" name="DAEND" size="16" class="form-control" required pattern="[0-9]{4}-[0-9]{2}-[0-9]{2}">
                                        </div>
                                    </div>
                                </div>



                                <div class="custom-box">
                                    <h3 style="text-align: center;">
                                        <button class="btn btn-theme" type="submit" style="font-size: medium;">Enregistrer</button>
                                        <a href="/home" class="btn btn-theme04" type="button" style="font-size: medium;">Annuler</a>
                                    </h3>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>

        </section>
    </section>








    <%- include("../partials/footer"); -%>