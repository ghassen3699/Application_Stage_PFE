<%- include("../partials/header"); -%>
    <section id="main-content">
        <section class="wrapper">



            <div class="row mt">
                <div class="col-lg-13">
                    <div class="custom-box">
                        <h3>navires > Les informations de la position</h3>
                    </div>
                    <div class="form-panel">


                        <div class="form">
                            <% positionData.forEach(position =>{ %>


                                <form class="cmxform form-horizontal style-form" method="post">
                                    <div class="form-group ">
                                        <label for="cemail" class="control-label col-lg-2" style="font-size: medium;">Nom du navire :</label>
                                        <div class="col-lg-5">
                                            <input value="<%= position['NA'] %>" class="form-control " id="curl" type="text" name="NA" disabled required />
                                        </div>
                                    </div>
                                    <br>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">ID_VMS du navire :</label>
                                        <div class="col-lg-10">
                                            <input value="<%= position['ID_VMS'] %>" class="form-control " id="curl" type="text" name="ID_VMS" disabled required />
                                        </div>
                                    </div>
                                    <br>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">RC du navire :</label>
                                        <div class="col-lg-5">
                                            <input value="<%= position['RC'] %>" class="form-control " id="curl" type="text" name="RC" disabled required />
                                        </div>
                                    </div>
                                    <br>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">Temps du position :</label>
                                        <div class="col-lg-5">
                                            <input value="<%= position['TI'] %>" class="form-control " id="curl" type="text" name="TI" required />
                                        </div>
                                    </div>
                                    <br>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">LT du position :</label>
                                        <div class="col-lg-10">
                                            <input value="<%= position['LT'] %>" class="form-control " id="curl" type="text" name="LT" required />
                                        </div>
                                    </div>


                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">LG du position :</label>
                                        <div class="col-lg-10">
                                            <input value="<%= position['LG'] %>" class="form-control " id="curl" type="text" name="LG" required />
                                        </div>
                                    </div>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">CO du position :</label>
                                        <div class="col-lg-10">
                                            <input value="<%= position['CO'] %>" class="form-control " id="curl" type="text" name="CO" required />
                                        </div>
                                    </div>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">SP du position :</label>
                                        <div class="col-lg-10">
                                            <input value="<%= position['SP'] %>" class="form-control " id="curl" type="text" name="SP" required />
                                        </div>
                                    </div>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">COM du position :</label>
                                        <div class="col-lg-10">
                                            <input value="<%= position['COM'] %>" class="form-control " id="curl" type="text" name="COM" required />
                                        </div>
                                    </div>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">TM du position :</label>
                                        <div class="col-lg-10">
                                            <input value="<%= position['TM'] %>" class="form-control " id="curl" type="text" name="TM" required />
                                        </div>
                                    </div>

                                    <div class="form-group ">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">Adresse IP du navire :</label>
                                        <div class="col-lg-10">
                                            <input value="<%= position['IPADDRESS'] %>" class="form-control " id="curl" type="text" name="IPADDRESS" required />
                                        </div>
                                    </div>


                                    <div class="form-group">
                                        <label for="curl" class="control-label col-lg-2" style="font-size: medium;">Date du position :</label>
                                        <div class="col-md-3 col-xs-11">
                                            <div class="input-append date dpYears">
                                                <input type="date" value="<%= position['DA'][0] %><%= position['DA'][1] %><%= position['DA'][2] %><%= position['DA'][3] %>-<%= position['DA'][4] %><%= position['DA'][5] %>-<%= position['DA'][6] %><%= position['DA'][7] %>" id="bday" name="DA" size="16"
                                                    class="form-control" required pattern="[0-9]{4}-[0-9]{2}-[0-9]{2}">
                                            </div>
                                        </div>
                                    </div>


                                    <div class="custom-box">
                                        <h3 style="text-align: center;">
                                            <button class="btn btn-theme" type="submit" style="font-size: medium;">Enregistrer</button>
                                            <a href="/navire/navires" class="btn btn-theme04" type="button" style="font-size: medium;">Annuler</a>
                                        </h3>
                                    </div>

                                </form>
                                <%}) %>

                        </div>
                    </div>
                </div>
            </div>

        </section>
    </section>


    <%- include("../partials/footer"); -%>