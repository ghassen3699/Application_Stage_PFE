<%- include("../partials/header"); -%>
    <!--external css-->
    <link href="/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" href="/lib/advanced-datatable/css/DT_bootstrap.css" />
    <!-- Custom styles for this template -->
    <link href="/css/style.css" rel="stylesheet">
    <link href="/css/style-responsive.css" rel="stylesheet">


    <section id="main-content">
        <section class="wrapper">
            <div class="custom-box">
                <h3>Navires > Tous Les Navires</h3>
            </div>
            <div class="row mb">
                <!-- page start-->
                <div class="content-panel">
                    <div class="adv-table">
                        <br>
                        <table cellpadding="0" cellspacing="0" border="0" id="example" class="display table table-bordered">
                            <thead>
                                <tr>
                                    <th style="text-align: center;">NA</th>
                                    <th style="text-align: center;">ID_VMS</th>
                                    <th class="hidden-phone" style="text-align: center;">REG_ID</th>
                                    <th class="hidden-phone" style="text-align: center;">IMEI</th>
                                    <th class="hidden-phone" style="text-align: center;">ICCID</th>
                                    <th class="hidden-phone" style="text-align: center;">RC</th>
                                    <th class="hidden-phone" style="text-align: center;">KEY_AES</th>
                                    <th class="hidden-phone" style="text-align: center;">DABEG</th>
                                    <th class="hidden-phone" style="text-align: center;">DAEND</th>
                                    <% user.forEach(element => { %>
                                        <%if (element.ADMIN === 1){ %>

                                            <th class="hidden-phone" style="text-align: center;">Modifier</th>
                                            <th class="hidden-phone" style="text-align: center;">Supprmer</th>

                                            <%}%>
                                                <%})%>
                                </tr>
                            </thead>

                            <tbody>
                                <% navires.forEach(function(navire) { %>

                                    <tr class="gradeX">

                                        <td style="text-align: center;font-size: medium;">
                                            <a href="/navire/navire/<%= navire.ID_VMS%>" style="color:gray;">
                                                <%= navire.NA %>
                                            </a>
                                        </td>
                                        <td class="hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= navire.ID_VMS %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= navire.REG_ID %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= navire.IMEI %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= navire.ICCID %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= navire.RC %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= navire.KEY_AES %>
                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">

                                            <%= navire.DABeg[6]%>
                                                <%= navire.DABeg[7]%> -
                                                    <%= navire.DABeg[4]%>
                                                        <%= navire.DABeg[5]%> -
                                                            <%= navire.DABeg[0]%>
                                                                <%= navire.DABeg[1]%>
                                                                    <%= navire.DABeg[2]%>
                                                                        <%= navire.DABeg[3]%>

                                        </td>
                                        <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                            <%= navire.DAEnd[6]%>
                                                <%= navire.DAEnd[7]%> -
                                                    <%= navire.DAEnd[4]%>
                                                        <%= navire.DAEnd[5]%> -
                                                            <%= navire.DAEnd[0]%>
                                                                <%= navire.DAEnd[1]%>
                                                                    <%= navire.DAEnd[2]%>
                                                                        <%= navire.DAEnd[3]%>
                                        </td>
                                        <% user.forEach(element => { %>
                                            <%if (element.ADMIN === 1){ %>

                                                <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                                    <a href="/navire/modifier/<%= navire.ID %>" style="color: goldenrod;" class="fa fa-refresh" aria-hidden="true"></a>
                                                </td>
                                                <td class="center hidden-phone" style="text-align: center; font-size: medium;">
                                                    <a href="/navire/supprimer/<%= navire.ID %>" style="color: red;" class="fa fa-trash-o"></a>
                                                </td>
                                                <%}%>
                                                    <%})%>
                                    </tr>

                                    <% }); %>

                            </tbody>
                        </table>

                    </div>
                </div>
                <!-- page end-->
            </div>
            <!-- /row -->
            <!-- /wrapper -->
        </section>
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>

        <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js" defer></script>

        <script>
            $(document).ready(function() {
                $('#example').DataTable();
            });
        </script>



        <%- include("../partials/footer"); -%>